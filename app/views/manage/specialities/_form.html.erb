<%= semantic_form_for [:manage, @chair, @speciality] do |form| %>
  <%= form.inputs do %>
    <%= form.input :chair %>
    <%= form.input :name %>
    <%= form.enum_input :degree %>
    <%= form.input :qualification %>
  <% end %>

  <%= form.buttons do %>
    <%= form.commit_button %>
  <% end %>
<% end %>

