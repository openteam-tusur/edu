<% if current_user && !current_user.has_been_started? %>
  <div class="get_started">
    <ul>
      <li <%= "style='text-decoration: line-through;'" unless current_user.human.new_record? %> >
        <p>
          <%= link_to 'Заполните', current_user.human.new_record? ? new_human_path : edit_human_path %> Ваш профиль.</p>
      </li>

      <li>
        <p><%= link_to 'Уточните Ваш статус', profile_path %> в ТУСУРе.</p>
      </li>

      <li>
        <p>Узнайте больше <a href='#'>о портале.</a></p>
      </li>

    </ul>
  </div>
<% end %>

