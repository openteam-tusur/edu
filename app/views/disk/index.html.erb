<div class="publication_wrapper">

  <p><%= link_to t("#{resource_class.name.downcase}.Создать диск"), new_disk_path, :class => 'button' %></p>

  <% @disks.each do |d| %>
    Год: <%= @year = d.year %>
    Месяц: <%=  @month = d.month %>
    Создано: <%= @created_at = d.created_at %>
    <%= link_to t("#{resource_class.name.downcase}.show"), disk_path(d.id), :class => 'button' %></br>
  <% end %>

</div>

